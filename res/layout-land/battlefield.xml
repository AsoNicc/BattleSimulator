<?xml version="1.0" encoding="utf-8"?><FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"     android:id="@+id/flScreen"     android:orientation="vertical"     android:layout_width="match_parent"     android:layout_height="match_parent" >    	    <ImageView android:id="@+id/ivBackground"         android:layout_width="match_parent"         android:layout_height="match_parent"         android:src="@drawable/battleground"         android:scaleType="fitXY"/>    	    <Game.Pokemon.Animated android:id="@+id/Animated"         android:layout_width="match_parent"         android:layout_height="match_parent"/>    	    <LinearLayout android:id="@+id/llActivity"         android:orientation="vertical"         android:layout_width="match_parent"         android:layout_height="match_parent"         android:weightSum="100" >        	        <LinearLayout android:id="@+id/llDebugWindow"             android:orientation="vertical"             android:layout_width="match_parent"             android:layout_height="match_parent"             android:layout_weight="33"             android:weightSum="4" >                        <LinearLayout android:id="@+id/llTextViewLayout"                android:orientation="vertical"                 android:layout_width="match_parent"                 android:layout_height="match_parent"                 android:layout_weight="3" >                                <TextView android:id="@+id/tvException"                     android:layout_width="match_parent"                     android:layout_height="wrap_content"                    android:text="@string/error_code"/>            </LinearLayout>                        <LinearLayout android:id="@+id/llCheerButtonLayout"                android:orientation="vertical"                 android:layout_width="wrap_content"                 android:layout_height="match_parent"                 android:layout_weight="1" >                                <Button android:id="@+id/bCheer"                     android:background="@drawable/cheer"                     android:layout_width="wrap_content"                     android:layout_height="wrap_content"                    android:backgroundTint="#95ffffff"                     android:backgroundTintMode="multiply"/>            </LinearLayout>    	        </LinearLayout>        	        <RelativeLayout android:id="@+id/rlDrawer"             android:orientation="vertical"             android:layout_width="match_parent"             android:layout_height="match_parent"             android:layout_weight="67" >                         <Game.Pokemon.WrappingSlidingDrawer android:id="@+id/WrappingSlidingDrawer"                 android:layout_width="match_parent"                 android:layout_height="match_parent"                 android:handle="@+id/bHandle"                 android:content="@+id/llDrawerContents" >                	                <Button android:id="@+id/bHandle"                     android:background="@drawable/frame_0"                     android:layout_width="64dp"                     android:layout_height="32dp"                     android:backgroundTint="#80ffffff"                     android:backgroundTintMode="multiply"/>                	                <LinearLayout android:id="@+id/llDrawerContents"                     android:orientation="vertical"                     android:background="#802f4f4f"                     android:layout_width="match_parent"                     android:layout_height="match_parent" >                    	                    <LinearLayout android:orientation="horizontal"                         android:layout_width="match_parent"                         android:layout_height="match_parent"                         android:weightSum="4" >                        	                        <Button android:id="@+id/bMoves"                             android:textColor="#2f4f4f"                             android:background="@drawable/silver_bubble"                             android:padding="0dp"                             android:layout_width="wrap_content"                             android:layout_height="match_parent"                             android:text="@string/moves"                             android:layout_weight="1"/>                        	                        <Button android:id="@+id/bSwap"                             android:textColor="#2f4f4f"                             android:background="@drawable/silver_bubble"                             android:padding="0dp"                             android:layout_width="wrap_content"                             android:layout_height="match_parent"                             android:text="@string/swap"                             android:layout_weight="1"/>                        	                        <Button android:id="@+id/bBuff"                             android:textColor="#2f4f4f"                             android:background="@drawable/silver_bubble"                             android:padding="0dp"                             android:layout_width="wrap_content"                             android:layout_height="match_parent"                             android:text="@string/buff"                             android:layout_weight="1"/>                        	                        <Button android:id="@+id/bForfeit"                             android:textColor="#2f4f4f"                             android:background="@drawable/silver_bubble"                             android:padding="0dp"                             android:layout_width="wrap_content"                             android:layout_height="match_parent"                             android:text="@string/forfeit"                             android:layout_weight="1"/>	                    </LinearLayout>	                </LinearLayout>             </Game.Pokemon.WrappingSlidingDrawer>	        </RelativeLayout>	    </LinearLayout></FrameLayout>