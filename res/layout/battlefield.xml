<?xml version="1.0" encoding="utf-8"?><FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"     android:orientation="vertical"     android:id="@+id/flScreen"     android:layout_width="match_parent"     android:layout_height="match_parent" >    	    <ImageView android:id="@+id/ivBackground"         android:layout_width="match_parent"         android:layout_height="match_parent"         android:src="res/drawable-ldpi/battleground.png"         android:scaleType="1"/>    	    <Game.Pokemon.Animated android:id="false"         android:layout_width="match_parent"         android:layout_height="match_parent"/>    	    <LinearLayout android:id="false"         android:orientation="1"         android:layout_width="match_parent"         android:layout_height="match_parent"         android:weightSum="{4:1120403456}" >        	        <LinearLayout android:id=""             android:orientation="1"             android:layout_width="match_parent"             android:layout_height="match_parent"             android:layout_weight="{4:1108082688}" >            	            <TextView android:id="@+id/tvException"                 android:layout_width="match_parent"                 android:layout_height="match_parent"                 android:text="Error 404"/>	        </LinearLayout>        	        <RelativeLayout android:id="false"             android:orientation="1"             android:layout_width="match_parent"             android:layout_height="match_parent"             android:layout_weight="{4:1115815936}" >                         <Game.Pokemon.WrappingSlidingDrawer android:id="false"                 android:layout_width="match_parent"                 android:layout_height="match_parent"                 android:handle="@+id/bHandle"                 android:content="@+id/llDrawerContents" >                	                <Button android:id="@+id/bHandle"                     android:background="res/drawable-ldpi/frame_0.png"                     android:layout_width="64dp"                     android:layout_height="32dp"                     android:backgroundTint="80ffffff"                     android:backgroundTintMode="14"/>                	                <LinearLayout android:id="@+id/llDrawerContents"                     android:orientation="1"                     android:background="802f4f4f"                     android:layout_width="match_parent"                     android:layout_height="match_parent"                     android:weightSum="{4:1073741824}" >                    	                    <LinearLayout android:id="@+id/llMovesAndBuff"                         android:orientation="0"                         android:layout_width="match_parent"                         android:layout_height="wrap_content"                         android:weightSum="{4:1073741824}"                         android:layout_weight="{4:1065353216}" >                        	                        <Button android:id="@+id/bMoves"                             android:textColor="2f4f4f"                             android:background="res/drawable-ldpi/silver_bubble.png"                             android:padding="0dp"                             android:layout_width="wrap_content"                             android:layout_height="match_parent"                             android:text="Moves"                             android:layout_weight="{4:1065353216}"/>                        	                        <Button android:id="@+id/bBuff"                             android:textColor="2f4f4f"                             android:background="res/drawable-ldpi/silver_bubble.png"                             android:padding="0dp"                             android:layout_width="wrap_content"                             android:layout_height="match_parent"                             android:text="Buff"                             android:layout_weight="{4:1065353216}"/>	                    </LinearLayout>                    	                    <LinearLayout android:id="@+id/llSwapAndForfeit"                         android:orientation="0"                         android:layout_width="match_parent"                         android:layout_height="wrap_content"                         android:weightSum="{4:1073741824}"                         android:layout_weight="{4:1065353216}" >                                                 <Button android:id="@+id/bSwap"                             android:textColor="2f4f4f"                             android:background="res/drawable-ldpi/silver_bubble.png"                             android:padding="0dp"                             android:layout_width="wrap_content"                             android:layout_height="match_parent"                             android:text="Swap"                             android:layout_weight="{4:1065353216}"/>                        	                        <Button android:id="@+id/bForfeit"                             android:textColor="2f4f4f"                             android:background="res/drawable-ldpi/silver_bubble.png"                             android:padding="0dp"                             android:layout_width="wrap_content"                             android:layout_height="match_parent"                             android:text="Forfeit"                             android:layout_weight="{4:1065353216}"/>	                    </LinearLayout>	                </LinearLayout>             </Game.Pokemon.WrappingSlidingDrawer>	        </RelativeLayout>	    </LinearLayout></FrameLayout>